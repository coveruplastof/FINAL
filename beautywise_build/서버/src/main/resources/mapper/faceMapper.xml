<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.last.application.mapper.faceMapper">
    <select id="selectFace" resultType="com.last.application.dto.faceDTO">
            SELECT face_shape,personal_color FROM face where face_id= #{face_id}
    </select>

    <insert id="createMember">
        insert into face(face_shape, personal_color) values(#{face_shape}, #{personal_color})
    </insert>

    <update id="updateMember">
        update face set face_shape = #{face_shape} , personal_color=#{personal_color} where face_id = #{face_id}
    </update>

</mapper>